{
  "name": "es2015-i18n-tag",
  "description": "ES2015 template literal tag for i18n and l10n translation and localization",
  "version": "0.7.1",
  "author": "Steffen Kolmer <steffen@kolmer.net>",
  "icon": "images/es2015-i18n-tag-icon.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/skolmer/es2015-i18n-tag.git"
  },
  "bugs": "http://github.com/skolmer/es2015-i18n-tag/issues",
  "engines": {
    "node": ">= 4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.11.1",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^1.8.1",
    "intl": "1.1.0",
    "mocha": "^2.5.3",
    "npm-run-all": "^2.1.1",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.1"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "test": "mocha --compilers js:babel-register",
    "clean": "rimraf dist/lib/**/*",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel lib -d dist/lib --source-maps --copy-files",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "lint": "eslint lib"
  },
  "main": "./dist/lib"
}
